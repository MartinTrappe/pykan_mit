{
    "project_name": "KAN_from_Scratch",
    "layer_dims": [2, 2, 1],
    "grid_size": 30,
    "spline_degree": 3,
    "learning_rate": 0.01,
    "num_steps": 1000,
    "dataset_size": 2000,

    "log_interval": 100,
    "use_regularization": false,
    "l1_weight": 0.001,
    "entropy_weight": 0.001,
    "update_strategy": "none",
    "adaptive_k": 2,
    "adaptive_patience": 300,
    "adaptive_threshold": 0.01,
    "fixed_update_interval": 200,

    "prune_rel_thresh": 0.1,
    "symbolify_r2_threshold": 0.99,
    "symbolify_samples_per_edge": 400,
    "freeze_after_symbolify": true,
    "lock_grids_after_symbolify": true,

    "ft_enabled": true,
    "ft_lr_scale": 0.2,
    "ft_steps": 200,
    "ft_l1_weight": 0.0015,
    "ft_entropy_weight": 0.0,
    "ft_prune_rel_thresh": 0.1,
    "ft_second_symbolify": true,
    "ft_r2_threshold": 0.995,
    "ft_samples_per_edge": 400,
    "ft_batch_size": "full",
    "ft_optimizer": "lbfgs",
    "ft_lr": 1.0,
    "ft_lbfgs_max_iter": 20,
    "ft_lbfgs_history_size": 10,
    "ft_log_interval": 200,
    "ft_heartbeat_secs": 45.0,
    "ft_ema_beta": 0.995,
    "ft_target_mse": 1.0e-4,
    "ft_patience_updates": 800,
    "ft_min_improve_rel": 1.0e-3,
    "ft_min_improve_abs": 5.0e-5
}

